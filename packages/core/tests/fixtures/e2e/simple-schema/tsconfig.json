{
        "compilerOptions": {
                "target": "ES2020",
                "module": "ES2020",
                "lib": ["ES2020"],
                "strict": true,
                "esModuleInterop": true,
                "skipLibCheck": true,
                "moduleResolution": "node",
                "resolveJsonModule": true,
                "declaration": true,
                "outDir": "./dist"
        },
        "include": ["src/**/*"]
}
